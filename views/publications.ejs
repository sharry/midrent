<div class="publications center">
	<% if (typeof(publications) !== "undefined") { %> <% for(var pub of
	publications) { %>
	<div class="publication">
		<div class="publisher">
			<div class="publisher-image">
				<img src="<%= pub.photoUrl %>" />
			</div>
			<span><%= pub.firstName%></span>
		</div>
		<% var saved = false;
			if (typeof(saves) !== "undefined") {
				for(var save of saves) {
					if (save.publicationId === pub.id)
						saved = true; 
				} 
			}
		%>
		<div class="save-pub" title="Enregistrer cette publication">
			<input
				type="checkbox"
				id="chk-save-pub"
				name="chk-save-pub"
