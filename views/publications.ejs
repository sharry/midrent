			<label for="chk-save-pub"></label>
		</div>
		<div class="carousel slide" data-bs-ride="carousel">
			<div class="carousel-inner">
				<% var first = true;
					for (var image of images) {
						if (image.localId === pub.localId) { %>
							<div class="carousel-item <% if(first){%>active<%}%>">
								<img
									src="<%= image.imageUrl%>"
									class="d-block w-100"
								/>
							</div>
							first = false;
